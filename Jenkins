    agent any
    tools {
    maven 'm1'
    }
    stages {
     stage('clone') {
     steps {
     checkout scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[credentialsId: 'ssh-key-fot-git1', url: 'https://github.com/Vikas-Anjanappa/b6-java.git']])
    }
  }

  stage('build') {
    steps {
      sh 'mvn clean install'
    }
  }

  stage('test') {
    steps {
      archiveArtifacts artifacts: 'target/**.jar', followSymlinks: false
      junit 'target/surefire-reports/*.*xml'
      }
  }

  stage('run') {
    steps {
      sh 'chmod 777 scripts/deliver.sh'
      sh './scripts/deliver.sh'
    }
  }

}
